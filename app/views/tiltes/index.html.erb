<p id="notice"><%= notice %></p>

<h1>Tiltes</h1>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tiltes.each do |tilte| %>
      <tr>
        <td><%= tilte.body %></td>
        <td><%= link_to 'Show', tilte %></td>
        <td><%= link_to 'Edit', edit_tilte_path(tilte) %></td>
        <td><%= link_to 'Destroy', tilte, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tilte', new_tilte_path %>
